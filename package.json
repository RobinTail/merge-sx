{
  "name": "merge-sx",
  "version": "0.1.0",
  "description": "Combines multiple SxProps for Material UI components.",
  "main": "dist/index.js",
  "author": {
    "name": "Anna Bocharova",
    "url": "https://robintail.cz"
  },
  "license": "MIT",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs",
      "iife"
    ],
    "splitting": false,
    "sourcemap": false,
    "minify": true,
    "clean": true,
    "dts": true
  },
  "scripts": {
    "build": "tsup",
    "test": "tsc --noEmit && jest"
  },
  "peerDependencies": {
    "@mui/material": "^5.1.0"
  },
  "devDependencies": {
    "@mui/material": "^5.10.12",
    "@swc/core": "^1.3.14",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.2.2",
    "jest": "^29.2.2",
    "react": "^17",
    "react-dom": "^17",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": [
        "@swc/jest"
      ]
    }
  }
}
